const log = require('logToConsole');
const injectScript = require('injectScript');
const copyFromWindow = require('copyFromWindow');
const callInWindow = require('callInWindow');
const setInWindow = require('setInWindow');
const accountId = data.accountId; 
log('data =', data);
var BKTAG = copyFromWindow('BKTAG');
setInWindow('bk_async',function() {
  setInWindow('bk_allow_multiple_calls', true, true);
  setInWindow('bk_use_multiple_iframes', true, true);
  callInWindow('BKTAG.doTag', accountId, 4);
},true);
const onSuccess = () => {
  data.gtmOnSuccess();
};
if (BKTAG) {
  onSuccess();
} else {
  const trackingUrl = 'https://tags.bkrtx.com/js/bk-coretag.js';
  injectScript(trackingUrl, onSuccess, data.gtmOnFailure, 'bkPx');
}


